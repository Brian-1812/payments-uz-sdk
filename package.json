{
  "name": "payments-uz-sdk",
  "version": "1.0.0",
  "description": "A comprehensive SDK for Uzbekistan's payment providers like Payme and Click.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:runner": "ts-node tests/run-tests.ts",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:click": "jest tests/unit/click tests/integration/ClickPaymentFlow",
    "test:payme": "jest tests/unit/payme tests/integration/PaymePaymentFlow",
    "test:errors": "jest tests/unit/errors",
    "test:utils": "jest tests/unit/utils tests/unit/click/utils tests/unit/click/status tests/unit/payme/utils"
  },
  "files": [
    "dist"
  ],
  "author": "Shohruhmirzo Toyirov",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "buffer": "^6.0.3"
  },
  "keywords": [
    "payme",
    "click",
    "uzbekistan",
    "payment",
    "sdk",
    "fintech"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/payments-uz-sdk.git"
  }
}
